<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_RIGHT" prefHeight="650.0" minWidth="155.0" BorderPane.alignment="CENTER_RIGHT"
      xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.hms.NavbarController">

    <Button fx:id="adminHome" alignment="CENTER" styleClass="menu-button" text="Home"
            onAction="#handleAdminHome">
        <VBox.margin>
            <Insets bottom="10.0" top="10.0" right="5.0"/>
        </VBox.margin>
    </Button>
    <Button fx:id="addEmployee" styleClass="menu-button" text="Add Employee"
            onAction="#handleAddEmployee">
        <VBox.margin>
            <Insets bottom="10.0" top="10.0" right="5.0"/>
        </VBox.margin>
    </Button>
    <Button fx:id="employeeInfo" styleClass="menu-button" text="Employee Info"
            onAction="#handleEmployeeInfo">
        <VBox.margin>
            <Insets bottom="10.0" top="10.0" right="5.0"/>
        </VBox.margin>
    </Button>
    <Button fx:id="customerInfo" styleClass="menu-button" text="Customer Info"
            onAction="#handleCustomerInfo">
        <VBox.margin>
            <Insets bottom="10.0" top="10.0" right="5.0"/>
        </VBox.margin>
    </Button>
    <Button fx:id="earningLog" alignment="CENTER" styleClass="menu-button" text="Earning Log"
            onAction="#handleEarningLog">
        <VBox.margin>
            <Insets bottom="10.0" top="10.0" right="5.0"/>
        </VBox.margin>
    </Button>
    <BorderPane.margin>
        <Insets bottom="10.0" top="10.0"/>
    </BorderPane.margin>
</VBox>



