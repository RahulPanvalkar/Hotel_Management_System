<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_RIGHT" prefHeight="650.0" minWidth="155.0" BorderPane.alignment="CENTER_RIGHT"
      xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.hms.NavbarController">

    <Button fx:id="home" alignment="CENTER" styleClass="menu-button" text="Home"
            onAction="#handleEmployeeHome">
        <VBox.margin>
            <Insets bottom="10.0" top="10.0" right="5.0"/>
        </VBox.margin>
    </Button>
    <Button fx:id="addEmployee" styleClass="menu-button" text="Room Check"
            onAction="#handleRoomCheck">
        <VBox.margin>
            <Insets bottom="10.0" top="10.0" right="5.0"/>
        </VBox.margin>
    </Button>
    <Button fx:id="employeeInfo" styleClass="menu-button" text="Check In"
            onAction="#handleEmployeeCheckIn">
        <VBox.margin>
            <Insets bottom="10.0" top="10.0" right="5.0"/>
        </VBox.margin>
    </Button>
    <Button fx:id="customerInfo" styleClass="menu-button" text="Check Out"
            onAction="#handleEmployeeCheckOut">
        <VBox.margin>
            <Insets bottom="10.0" top="10.0" right="5.0"/>
        </VBox.margin>
    </Button>
    <Button fx:id="earningLog" alignment="CENTER" styleClass="menu-button" text="Check History"
            onAction="#handleEmployeeCheckHistory">
        <VBox.margin>
            <Insets bottom="10.0" top="10.0" right="5.0"/>
        </VBox.margin>
    </Button>

    <BorderPane.margin>
        <Insets bottom="10.0" top="10.0"/>
    </BorderPane.margin>
</VBox>



